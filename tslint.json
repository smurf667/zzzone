{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "rules": {
    "ter-indent": [true, 2],
	"no-bitwise": false,
    "max-line-length": [true, {"limit": 120, "ignore-pattern": "(^.+data:(audio|image|video).+)|(^.+innerHTML.+)"}],
    "interface-name": [true, "never-prefix"]
  }
}